<?xml version="1.0"?>
<!--
<%- header %>
-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="after.body.start">
            <block name="<%= custom %>.block" class="<%= phpNamespace %>\Block\<%= blockName %>.php" template="<%= moduleClassName %>::<%= custom %>.phtml" />
        </referenceContainer>
        <referenceContainer name="before.body.end">
            <block name="<%= module %>.js" class="Magento\Framework\View\Element\Template" template="<%= moduleClassName %>::_scripts.phtml" />
        </referenceContainer>
    </body>
</page>