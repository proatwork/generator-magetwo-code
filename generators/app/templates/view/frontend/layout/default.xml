<?xml version="1.0"?>
<!--
<%- context.header_file %>
-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="after.body.start">
            <block name="<%= context.default_block_lc %>.block" class="<%= context.namespace %>\Block\<%= context.default_block %>" template="<%= context.class_name %>::<%= context.default_block_lc %>.phtml" />
        </referenceContainer>
        <referenceContainer name="before.body.end">
            <block name="<%= context.module_lc %>.js" class="Magento\Framework\View\Element\Template" template="<%= context.class_name %>::_scripts.phtml" />
        </referenceContainer>
    </body>
</page>
